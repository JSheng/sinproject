{
  "name": "iind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:win": "concurrently \"npm run dev:client\" \"npm run dev:server\" ",
    "dev:server": "cross-env WEBPACK_TARGET=node node ./src/ssr/ssr.js -mode development",
    "dev:client": "vue-cli-service serve -mode development",
    "build:client": "vue-cli-service build -mode production",
    "build:server": "cross-env WEBPACK_TARGET=node vue-cli-service build --mode server",
    "build:win": "npm run build:server && move package\\dist\\vue-ssr-server-bundle.json bundle && npm run build:client && move bundle package\\dist\\vue-ssr-server-bundle.json"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "axios": "^0.19.0",
    "core-js": "^2.6.10",
    "iview": "^3.5.1",
    "koa": "^2.8.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "generate-asset-webpack-plugin": "^0.3.0",
    "image-webpack-loader": "^6.0.0",
    "lodash.merge": "^4.6.2",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.12.0",
    "pngquant-bin": "^5.0.2",
    "sass-loader": "^8.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.0",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "last 10 Chrome versions",
    "last 5 Firefox versions",
    "Safari >= 6",
    "ie > 8"
  ]
}
